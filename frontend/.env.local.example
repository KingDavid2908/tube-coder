# These URLs should point to your running backend server.
# The defaults assume the backend runs on port 8080.

# Base URL for the backend API server (used if specific endpoints aren't set)
NEXT_PUBLIC_BACKEND_API_URL=http://localhost:8080

# Full URL for the chat endpoint
NEXT_PUBLIC_BACKEND_CHAT_API_URL=http://localhost:8080/api/chat

# Full URL for the cancellation endpoint
NEXT_PUBLIC_BACKEND_CANCEL_API_URL=http://localhost:8080/api/cancel_request

# Full URL for the project download endpoint
NEXT_PUBLIC_BACKEND_DOWNLOAD_URL=http://localhost:8080/api/download_project

